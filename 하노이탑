//<하노이탑>
#include <stdio.h>

/**
 * @brief 재귀적으로 하노이 탑 이동 순서를 출력합니다.
 *
 * @param n     이동할 원판의 수
 * @param from  막대 A (출발 기둥 라벨)
 * @param tmp   막대 B (보조 기둥 라벨)
 * @param to    막대 C (목표 기둥 라벨)
 */
void hanoi_tower(int n, char from, char temp, char to) {
    // To Fill
    if(n==1)
        printf("[1] : %c -> %c \n", from, to);
    else {
        hanoi_tower(n-1, from, to, temp);
        printf("[%d] : %c -> %c \n", n, from, to);
        hanoi_tower(n-1, temp, from, to);
    }
}

int main(void) {
    int n;

    // 원판 개수 입력
    if (scanf("%d", &n) != 1 || n <= 0) {
        fprintf(stderr, "올바른 원판 개수를 입력하세요.\n");
        return 1;
    }

    // A 기둥 → C 기둥으로 하노이 탑 이동
    hanoi_tower(n, 'A', 'B', 'C');
    return 0;
}
